Cell[CellGroupData[{

Cell["Distribution of \[CapitalDelta]I for given \[CapitalDelta]p", "Section",
 CellChangeTimes->{{3.7078215200623217`*^9, 3.7078215334716625`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Yp", "=", 
   RowBox[{"NormalDistribution", "[", 
    RowBox[{"\[Mu]off", ",", " ", "\[Sigma]off"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fYp", "[", "x_", "]"}], ":=", 
   FractionBox[
    RowBox[{"PDF", "[", 
     RowBox[{"Yp", ",", " ", "x"}], "]"}], 
    RowBox[{"1", "-", 
     RowBox[{"CDF", "[", 
      RowBox[{"Yp", ",", " ", 
       RowBox[{"k", "-", "1"}]}], "]"}]}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"fYp", "[", "x", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<Normalized? \>\"", ",", " ", 
    RowBox[{"Assuming", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Mu]off", " ", "\[Element]", " ", "Reals"}], ",", " ", 
        RowBox[{"\[Sigma]off", " ", "\[Element]", " ", "Reals"}], ",", " ", 
        RowBox[{"\[Sigma]off", " ", ">", "0"}]}], "}"}], ",", " ", 
      RowBox[{"FullSimplify", "@", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"fYp", "[", "x", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"k", "-", "1"}], ",", " ", "\[Infinity]"}], "}"}]}], 
        "]"}]}]}], "]"}]}], "\[IndentingNewLine]", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"thisk", "=", "5"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"fYp", "[", "x", "]"}], " ", "/.", " ", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"k", "\[Rule]", "thisk"}], ",", " ", 
      RowBox[{"\[Mu]off", " ", "\[Rule]", " ", "6"}], ",", " ", 
      RowBox[{"\[Sigma]off", " ", "\[Rule]", " ", "2"}]}], "}"}]}], ",", 
