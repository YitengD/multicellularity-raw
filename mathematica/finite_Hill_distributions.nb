  3.7089409034203153`*^9, 3.708940909925515*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", 
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"k", " ", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", 
             FractionBox["u", 
              RowBox[{
               RowBox[{"-", "1"}], "+", "u"}]]}], ")"}], 
           FractionBox["1", "n"]]}], "+", "\[Mu]"}], ")"}], "2"], 
      RowBox[{"2", " ", 
