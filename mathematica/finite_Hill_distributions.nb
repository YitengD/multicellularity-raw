Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"params", " ", "=", "  ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"k", "\[Rule]", "16"}], ",", " ", 
     RowBox[{"n", "\[Rule]", " ", "2"}], ",", " ", 
     RowBox[{"fN", "\[Rule]", "3"}], ",", " ", 
     RowBox[{"Con", "\[Rule]", "20"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Mu]0", " ", "=", "0.5"}], ";", " ", 
   RowBox[{"\[Sigma]0", "=", "0.25"}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Px", "[", "u_", "]"}], ":=", " ", 
   RowBox[{
    FractionBox["k", 
     RowBox[{"n", " ", "u", " ", 
      RowBox[{"(", 
       RowBox[{"1", "-", "u"}], ")"}]}]], 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox["u", 
       RowBox[{"1", "-", "u"}]], ")"}], 
     RowBox[{"1", "/", "n"}]], 
    RowBox[{"PDF", "[", 
     RowBox[{
      RowBox[{"NormalDistribution", "[", 
       RowBox[{"\[Mu]", ",", "\[Sigma]"}], "]"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         FractionBox["u", 
          RowBox[{"1", "-", "u"}]], ")"}], "^", 
        RowBox[{"(", 
         RowBox[{"1", "/", "n"}], ")"}]}], " ", "k"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"f", "[", 
     RowBox[{"\[Mu]old_", ",", "\[Sigma]old_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "x2new", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[Mu]Y", " ", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Mu]old", "+", "fN"}], ")"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"Con", "-", "1"}], ")"}], "\[Mu]old"}], "-", "1"}], 
           ")"}]}], " ", "/.", " ", "params"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"\[Sigma]Y", " ", "=", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"fN", "^", "2"}], 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"Con", "-", "1"}], ")"}], "^", "2"}]}], "+", "1"}], 
           "]"}], "\[Sigma]old"}], " ", "/.", " ", "params"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"\[Mu]new", " ", "=", " ", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"u", "*", 
             RowBox[{"Px", "[", "u", "]"}]}], " ", "/.", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"\[Mu]", "\[Rule]", " ", "\[Mu]Y"}], ",", " ", 
              RowBox[{"\[Sigma]", "\[Rule]", " ", "\[Sigma]Y"}]}], "}"}]}], 
           " ", "/.", " ", "params"}], ",", 
          RowBox[{"{", 
           RowBox[{"u", ",", "0", ",", "1"}], "}"}], ",", " ", 
          RowBox[{"AccuracyGoal", "\[Rule]", " ", "20"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"x2new", " ", "=", " ", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"u", "^", "2"}], "*", 
             RowBox[{"Px", "[", "u", "]"}]}], " ", "/.", 
            RowBox[{"{", 
