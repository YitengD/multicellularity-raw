Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "\[Mu]", ",", "\[Sigma]", ",", " ", "\[Mu]X", ",", "\[Sigma]X", ",", "fN", 
    ",", " ", "n", ",", "k"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Pz2", "[", "z_", "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"PDF", "[", 
       RowBox[{
        RowBox[{"NormalDistribution", "[", 
         RowBox[{"\[Mu]", ",", "\[Sigma]"}], "]"}], ",", 
        RowBox[{"z", "^", 
         RowBox[{"(", 
          RowBox[{"1", "/", "n"}], ")"}]}]}], "]"}], "+", 
      RowBox[{"PDF", "[", 
       RowBox[{
        RowBox[{"NormalDistribution", "[", 
         RowBox[{"\[Mu]", ",", "\[Sigma]"}], "]"}], ",", 
        RowBox[{"-", 
         RowBox[{"z", "^", 
          RowBox[{"(", 
           RowBox[{"1", "/", "n"}], ")"}]}]}]}], "]"}]}], ")"}], "/", 
    RowBox[{"(", 
     RowBox[{"n", "*", 
      RowBox[{"z", "^", 
       RowBox[{"(", 
        FractionBox[
         RowBox[{"n", "-", "1"}], "n"], ")"}]}]}], ")"}]}]}], ";", 
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"Pz2", "[", "x", "]"}], "*", 
    RowBox[{"DiracDelta", "[", 
     RowBox[{"x", "+", 
      RowBox[{"k", "^", "n"}], " ", "-", " ", "y"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"sol", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Abs", "[", "z", "]"}], " ", 
     RowBox[{"f", "[", 
      RowBox[{
       RowBox[{"u", " ", "z"}], ",", "z"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"z", ",", 
      RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Assumptions", "\[Rule]", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"k", "^", "n"}], "\[Element]", "Reals"}], ",", 
       RowBox[{"k", "\[Element]", "Reals"}], ",", 
       RowBox[{"u", " ", "\[Element]", "Reals"}], ",", " ", 
       RowBox[{"n", "\[Element]", "Reals"}], ",", 
       RowBox[{"x", "\[Element]", "Reals"}], ",", 
       RowBox[{"y", "\[Element]", "Reals"}], ",", " ", 
       RowBox[{"k", ">", "0"}], ",", " ", 
       RowBox[{"n", ">", "0"}], ",", " ", 
       RowBox[{"\[Mu]", ">", "0"}], ",", " ", 
       RowBox[{"\[Sigma]", ">", "0"}]}], "}"}]}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.708936334291852*^9, 3.7089363434178514`*^9}, {
  3.708936378049852*^9, 3.708936449622652*^9}, {3.708936486391852*^9, 
  3.7089365004474516`*^9}, {3.7089376210848517`*^9, 3.7089376461384516`*^9}, {
  3.7089381892056518`*^9, 3.708938191498852*^9}, {3.7089405634807153`*^9, 
